function dealDelay()
	for i = 1, 60 do
		coroutine.yield(0)
	end
end

function dealBag()
	local oneBag = getObjectFromGUID(shuffleOneBag[1])
	local oneBagQuantity = oneBag.getObjects()
	local fiveBag = getObjectFromGUID(shuffleFiveBag[1])
	local fiveBagQuantity = fiveBag.getObjects()
	local twentyBag = getObjectFromGUID(shuffleTwentyBag[1])
	local twentyBagQuantity = twentyBag.getObjects()
	local o = 0
	local f = 0
	local t = 0
	for k,v in pairs(oneBagQuantity) do
		o = o+1
	end
	for k,v in pairs(fiveBagQuantity) do
		f = f+1
	end
	for k,v in pairs(twentyBagQuantity) do
		t = t+1
	end
    	    oneBag.shuffle()
	    fiveBag.shuffle()
	    twentyBag.shuffle()
	    local oneHeightInc = 1
	    local fiveHeightInc = 1
	    local twentyHeightInc = 1
	    for i = 1, o do
	        oneBag.takeObject({position = {-10, oneHeightInc, -2}, rotation = {0, 180, 180}})
	        oneHeightInc = oneHeightInc + 0.3
	        oneBag.destruct()
	    end
	    for i = 1, f do
	        fiveBag.takeObject({position = {-10, fiveHeightInc, -6}, rotation = {0, 180, 180}})
	        fiveHeightInc = fiveHeightInc + 0.3
	        fiveBag.destruct()
	    end
	    for i = 1, t do
	        twentyBag.takeObject({position = {-10, twentyHeightInc, -10}, rotation = {0, 180, 180}})
	        twentyHeightInc = twentyHeightInc + 0.3
	        twentyBag.destruct()
	end
end

