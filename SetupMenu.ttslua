function selectBoomer1()
	if playerOneSelected == "Boomer" then
		clearPlayerOneSelection()
	elseif (playerTwoSelected  == "Boomer") or (playerThreeSelected  == "Boomer") or (playerFourSelected  == "Boomer") then
		broadcastToAll ( "Hero already selected by another player", {1, 0.5, 0.5} )
	else
    Boomer1.label = ("* Boomer *")
    Ghillie1.label = ("Ghillie")
    Nugget1.label = ("Nugget")
    Patches1.label = ("Patches")
    Picket1.label = ("Picket")
